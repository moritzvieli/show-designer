{
  "name": "show-designer",
  "version": "1.0.0",
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretty-quick": "pretty-quick --staged",
    "precommit": "npm run pretty-quick && npm run lint",
    "copy_lib_assets": "ncp src/assets/designer projects/designer/dist/assets",
    "build_lib": "ng build --configuration production designer && npm run copy_lib_assets",
    "npm_pack": "cd projects/designer/dist && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "npm run package && cd projects/designer/dist && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/localize": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "bootstrap": "^4.1.2",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "ncp": "^2.0.0",
    "ngx-bootstrap": "^6.2.0",
    "ngx-bootstrap-slider": "^2.1.0",
    "ngx-dropzone-wrapper": "^10.0.1",
    "ngx-sortablejs-plus": "^15.0.2",
    "ngx-toastr": "^13.2.1",
    "rmdir-cli": "^2.0.6",
    "rxjs": "^6.2.2",
    "sortablejs": "^1.15.2",
    "split.js": "^1.3.5",
    "three": "^0.165.0",
    "tslib": "^2.0.0",
    "wavesurfer.js": "^3.0.0",
    "web-audio-beat-detector": "^8.0.35",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-devkit/build-ng-packagr": "^0.1002.4",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "awesome-typescript-loader": "2.2.1",
    "codelyzer": "^5.1.2",
    "eslint": "^8.17.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.2.2",
