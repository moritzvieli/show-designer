{
  "name": "show-designer",
  "version": "1.0.0",
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-lib-assets": "cpx \"projects/designer/assets\" \"projects/designer/dist\"",
    "build_lib": "ng build --prod designer && npm run copy-lib-assets",
    "npm_pack": "cd projects/designer/dist && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "npm run package && cd projects/designer/dist && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
